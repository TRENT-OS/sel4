				power-controller-vpu {
					compatible = "amlogic,meson-gx-pwrc-vpu";
					#power-domain-cells = <0x00>;
					amlogic,hhi-sysctrl = <0x15>;
					resets = <0x14 0x05 0x14 0x0a 0x14 0x0d 0x14 0x25 0x14 0x84 0x14 0x85 0x14 0x86 0x14 0x87 0x14 0x89 0x14 0x8c 0x14 0x8d 0x14 0xe7>;
