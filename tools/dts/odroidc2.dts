				clock-names = "clkin\0core\0adc_clk\0adc_sel";
				vref-supply = <0x13>;
			};

			pwm@86c0 {
				compatible = "amlogic,meson-gx-pwm\0amlogic,meson-gxbb-pwm";
				reg = <0x00 0x86c0 0x00 0x10>;
